  },
  "homepage": "https://github.com/timsmith78/nytreact#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^5.3.0",
    "request": "^2.88.0"
  }
}